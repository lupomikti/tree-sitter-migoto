{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "migoto",
      "camelcase": "Migoto",
      "title": "Migoto",
      "scope": "source.ini.3dm",
      "file-types": [
        "ini"
      ],
      "injection-regex": "^migoto$",
      "class-name": "TreeSitterMigoto"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "DSL for the INI files used by 3Dmigoto",
    "authors": [
      {
        "name": "AGMG",
        "url": "https://discord.gg/AGMG"
      }
    ],
    "links": {
      "repository": "https://github.com/lupomikti/tree-sitter-migoto"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}